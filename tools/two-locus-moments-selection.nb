    ";"}], "\[IndentingNewLine]", 
   RowBox[{
    RowBox[{"cs", " ", "=", " ", 
     RowBox[{"{", 
      RowBox[{"c1", ",", " ", "c2", ",", " ", "c3", ",", " ", "c4"}], "}"}]}],
     ";"}], " ", 
   RowBox[{"(*", " ", 
    RowBox[{
